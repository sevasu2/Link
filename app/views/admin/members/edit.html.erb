<% @page_title = "会員情報の編集" %>

<h1><%= @page_title %></h1>

<div class="toolbar gradient-btn"><%= link_to "会員の詳細に戻る", [:admin, @member] %></div>

<div class="margin-top">
	<%= form_for [:admin, @member] do |form| %>
  <%= render "shared/member_form", form: form %>
  <div class="event-submit"><%= form.submit %></div>
<% end %>
</div>

